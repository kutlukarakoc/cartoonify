{
  "expo": {
    "name": "Cartoonify - Ghibli",
    "slug": "cartoonify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cartoonify.app"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share with AI.",
          "cameraPermission": "The app accesses your camera to let you take photos and share with AI."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}